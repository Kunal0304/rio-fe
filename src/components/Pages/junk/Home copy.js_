function SampleNextArrow(props) {
    const { className, style, onClick } = props;
    return ( <
        div className = { className }
        style = {
            {...style, display: "block", background:"transparent" } }
        onClick = { onClick }
        />
    );
}

function SamplePrevArrow(props) {
    const { className, style, onClick } = props;
    return ( <
        div className = { className }
        style = {
            {...style, display: "block",background:"transparent"   } }
        onClick = { onClick }
        />
    );
}
